<?xml version='1.0' encoding='utf-8'?>
<widget id="br.gov.sp.prodam.dba_app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>De Braços Abertos</name>
    <description>
        De Braços Abertos plataforma de cadastramento.
    </description>
    <author email="" href="http://www.prodam.sp.gov.br">
        GRX - PRODAM
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="orientation" value="portrait" />
	<preference name="android-minSdkVersion" value="14" />
	<engine name="android" spec="~4.1.1" />
</widget>

