<?xml version='1.0' encoding='utf-8'?>
<widget id="br.gov.sp.prodam.dba_app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.liteglue.SQLitePlugin" />
    </feature>
    <feature name="Datepicker">
        <param name="android-package" value="nl.moreapps.datepicker.Datepicker" />
    </feature>
    <allow-intent href="market:*" />
    <name>De Braços Abertos</name>
    <description>
        De Braços Abertos plataforma de cadastramento.
    </description>
    <author email="" href="http://www.prodam.sp.gov.br">
        GRX - PRODAM
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
